{
  "name": "spec-driven-nx-template",
  "private": true,
  "packageManager": "pnpm@9",
  "scripts": {
    "nx:init": "pnpm dlx nx@latest init --nxCloud=false --pm=pnpm",
    "gen:openapi": "tsx scripts/gen-openapi.ts",
    "gen:asyncapi": "tsx scripts/gen-asyncapi.ts",
    "spec:plan": "spec-kit plan",
    "spec:openapi": "spec-kit openapi",
    "spec:asyncapi": "spec-kit asyncapi",
    "codegen": "pnpm spec:plan && pnpm spec:openapi && pnpm spec:asyncapi && pnpm gen:openapi && pnpm gen:asyncapi",
    "local:up": "docker compose up -d localstack",
    "local:down": "docker compose down",
    "local:bootstrap": "scripts/local/bootstrap-localstack.sh",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "test:contract": "jest --testPathPattern=contract",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "build": "nx run-many -t build"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "@aws-sdk/client-eventbridge": "^3.0.0",
    "@aws-sdk/client-sqs": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/jest": "^29.5.0",
    "@nx/jest": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@nx/node": "21.5.3",
    "@nx/react": "21.5.3",
    "@nx/next": "21.5.3",
    "@nx/eslint": "21.5.3",
    "@nx/js": "21.5.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "nx": "21.5.3",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "prettier": "^3.2.0"
  }
}